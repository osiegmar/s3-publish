{
  "name": "s3-publisher-action",
  "version": "1.1.0",
  "private": true,
  "description": "Publish files to S3 by maintaining file (glob) specific metadata",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "all": "npm run build && npm run format && npm run lint && npm run package"
  },
  "keywords": [],
  "author": "Oliver Siegmar",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@aws-sdk/client-s3": "^3.701.0",
    "@aws-sdk/lib-storage": "^3.701.0",
    "@types/async": "^3.2.24",
    "async": "^3.2.6",
    "mime-types": "^2.1.35",
    "minimatch": "^10.0.1",
    "s3-etag": "^1.0.5"
  },
  "devDependencies": {
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.10.1",
    "@vercel/ncc": "^0.38.3",
    "eslint": "^8.57.0",
    "eslint-plugin-github": "^5.1.3",
    "eslint-plugin-jest": "^28.9.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  }
}
